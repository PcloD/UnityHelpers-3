    <TestProjectType>UnitTest</TestProjectType>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)' == 'Debug'">
    <DocumentationFile />
  </PropertyGroup>
  <ItemGroup>
    <Reference Include="System" />
